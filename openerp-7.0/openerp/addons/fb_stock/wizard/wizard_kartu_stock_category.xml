<?xml version="1.0" encoding="utf-8"?>
<openerp>
    <data>

        <record id="form_fb_stock_wizardKartuStockCategory" model="ir.ui.view">
            <field name="name">Form Wizard Kartu Stock Category</field>
            <field name="model">fb_stock.wizard_kartu_stock_category</field>
            <field name="type">form</field>
            <field name="arch" type="xml">
                <form string="Cetak Kartu Stock Berdasarkan Category ">
					<field name="date_from"/>
					<newline/>
					<field name="date_to"/>
					<newline/>
						<button special="cancel" string="Cancel" icon="gtk-cancel"/>
						<button name="cetak_laporan" type="object" string="Print" icon="gtk-print"/>

                </form>
            </field>
        </record>

        <record id="waction_fb_stock_wizardKartuStockCategory" model="ir.actions.act_window">
            <field name="name">Cetak Kartu Stock Category</field>
            <field name="res_model">fb_stock.wizard_kartu_stock_category</field>
            <field name="type">ir.actions.act_window</field>
            <field name="view_type">form</field>
            <field name="view_mode">form</field>
			<field name="view_id" ref="form_fb_stock_wizardKartuStockCategory"/>
            <field name="target">new</field>
        </record>

		<menuitem
			id="menu_fb_stock_menuKartuStockCategory"
			parent="stock.next_id_61"
			action="waction_fb_stock_wizardKartuStockCategory"
			name="Kartu Stock Category" />
    </data>
</openerp>
