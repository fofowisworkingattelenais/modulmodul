<?xml version="1.0" encoding="utf-8"?>
<openerp>
    <data>
        <record id="form_hr_wizardCetakF2a" model="ir.ui.view">
            <field name="name">Form Wizard Cetak F2a</field>
            <field name="model">hr.wizard_cetak_f2a</field>
            <field name="type">form</field>
            <field name="arch" type="xml">
				<form string="Cetak Formulir F2a">
					<field name="bulan_id" widget="selection"/>
					<field name="tahun"/>
					<button name="cetak" string="Cetak" type="object" icon="gtk-print" colspan="2"/>
					<button special="cancel" string="Batal" icon="gtk-cancel" colspan="2"/>
				</form>
            </field>
        </record>
        
        <record id="waction_hr_wizardCetakF2a" model="ir.actions.act_window">
            <field name="name">Cetak Formulir F2a</field>
            <field name="res_model">hr.wizard_cetak_f2a</field>
            <field name="view_type">form</field>
            <field name="view_mode">form</field>
            <field name="target">new</field>
        </record>        
    </data>
</openerp>
