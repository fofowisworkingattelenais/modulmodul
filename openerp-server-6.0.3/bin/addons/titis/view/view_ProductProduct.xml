<?xml version="1.0" encoding="utf-8"?>
<openerp>
    <data>
         <record id="tree_titis_product" model="ir.ui.view">
         	<field name="name">Tree Product : Titis Sampurna</field>
            <field name="model">product.product</field>
            <field name="type">tree</field>
            <field name="inherit_id" ref="product.product_product_tree_view"/>
            <field name="arch" type="xml">
                <data>
                	<field name="name" position="after">
						<field name="kategori_lama"/>
					</field>
				</data>
            </field>
        </record>
        
         <record id="search_titis_product" model="ir.ui.view">
         	<field name="name">Search Product : Titis Sampurna</field>
            <field name="model">product.product</field>
            <field name="type">search</field>
            <field name="inherit_id" ref="product.product_search_form_view"/>
            <field name="arch" type="xml">
                <data>
                	<field name="name" position="after">
						<field name="kategori_lama"/>
					</field>
				</data>
            </field>
        </record>        
    </data>
</openerp>

