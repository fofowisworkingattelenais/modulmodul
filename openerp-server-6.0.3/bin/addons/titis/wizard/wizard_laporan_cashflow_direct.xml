<?xml version="1.0" encoding="utf-8"?>
<openerp>
    <data>

        <record id="form_titis_wizardLaporanCashFlowDirect" model="ir.ui.view">
            <field name="name">Form Wizard Laporan CashFlow Direct</field>
            <field name="model">titis.wizard_laporan_cashflow_direct</field>
            <field name="type">form</field>
            <field name="arch" type="xml">
                <form string="Report Laporan CashFlow Direct">
					<group name="group_1" colspan="4" col="2">
						<group name="group_1_1" colspan="1" col="2">
							<field name="fiscal_year_id" colspan="2"/>
							<field name="account_id" colspan="2"/>
						</group>											
					</group>
					<button special="cancel" string="Cancel" icon="gtk-cancel" colspan="2"/>
					<button name="print_report" type="object" string="Print" colspan="2" icon="gtk-print"/>
                </form>
            </field>
        </record>

        <record id="waction_titis_wizardLaporanCashFlowDirect" model="ir.actions.act_window">
            <field name="name">CashFlow Direct</field>
            <field name="res_model">titis.wizard_laporan_cashflow_direct</field>
            <field name="type">ir.actions.act_window</field>
            <field name="view_type">form</field>
            <field name="view_mode">form</field>
			<field name="view_id" ref="form_titis_wizardLaporanCashFlowDirect"/>
            <field name="target">new</field>
        </record>
    </data>
</openerp>
