			<field name="view_id" ref="tree_base_negara"/>
			<field name="act_window_id" ref="waction_base_negara"/>
		</record>

		<record id="wactionView_base_formNegara" model="ir.actions.act_window.view">
			<field eval="1" name="sequence"/>
			<field name="view_mode">form</field>
			<field name="view_id" ref="form_base_negara"/>
