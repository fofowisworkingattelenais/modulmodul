<?xml version="1.0" encoding="utf-8"?>
<openerp>
    <data>
         <record id="tree_pendidikan_jenisJenjangPendidikan" model="ir.ui.view">
            <field name="name">Tree Jenis Jenjang Pendidikan</field>
            <field name="model">pendidikan.jenis_jenjang_pendidikan</field>
            <field name="type">tree</field>
            <field name="arch" type="xml">
				<tree string = "Jenis Jenjang Pendidikan">
                    <field name="kode"/>
					<field name="name"/>
				</tree>
			</field>
        </record>
        
        
         <record id="form_pendidikan_jenisJenjangPendidikan" model="ir.ui.view">
            <field name="name">Form Jenis Jenjang Pendidikan</field>
            <field name="model">pendidikan.jenis_jenjang_pendidikan</field>
            <field name="type">form</field>
            <field name="arch" type="xml">
				<form string = "Jenis Jenjang Pendidikan">
                    <field name="kode"/>
					<field name="name"/>
				</form>
			</field>
        </record>
    </data>
</openerp>
