<?xml version="1.0" encoding="utf-8"?>
<openerp>
    <data>
        <record id="tree_pendidikan_jenjangPendidikan" model="ir.ui.view">
            <field name="name">Tree Jenjang Pendidikan</field>
            <field name="model">pendidikan.jenjang_pendidikan</field>
            <field name="type">tree</field>
            <field name="arch" type="xml">
               <tree string = "Jenjang Pendidikan">
                    <field name="kode"/>
                    <field name="name"/>
				</tree>
			</field>
        </record>
        
        <!-- Roo7 -->
        
        <record id="form_pendidikan_jenjangPendidikan" model="ir.ui.view">
            <field name="name">Form Jenjang Pendidikan</field>
            <field name="model">pendidikan.jenjang_pendidikan</field>
            <field name="type">form</field>
            <field name="arch" type="xml">
				<form string = "Jenjang Pendidikan">
                    <field name="kode"/>
                    <field name="name"/>
				 	<field name="jenis_jenjang_id"/>
				</form>
			</field>
        </record>
    </data>
</openerp>
